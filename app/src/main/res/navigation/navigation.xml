<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/titleFragment">

    <fragment
        android:id="@+id/titleFragment"
        android:name="com.kml.parliamentapp.title.TitleFragment"
        android:label="TitleFragment"
        tools:layout="@layout/fragment_title"
        >
        <action
            android:id="@+id/action_titleFragment_to_parliamentMemberFragment"
            app:destination="@id/parliamentMemberFragment" />
    </fragment>
    <fragment
        android:id="@+id/parliamentMemberFragment"
        android:name="com.kml.parliamentapp.member.MemberFragment"
        android:label="ParliamentMemberFragment"
        tools:layout="@layout/fragment_member" />
</navigation>